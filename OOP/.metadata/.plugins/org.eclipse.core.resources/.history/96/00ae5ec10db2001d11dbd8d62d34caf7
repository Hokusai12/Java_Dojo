
public class ZookeeperTest {

	public static void main(String[] args) {
		//Gorilla Test
		Gorilla cutie = new Gorilla();
		System.out.println("-----Gorilla Test-----");
		
		cutie.throwObject();
		cutie.throwObject();
		cutie.throwObject();
		
		cutie.eatBanana();
		cutie.eatBanana();
		
		cutie.climb();
		
		cutie.displayEnergy();
		System.out.println("-----Bat Test-----");
		Bat vampire = new Bat();
		
		vampire.attackTown();
		vampire.attackTown();
		vampire.attackTown();
		
		vampire.eatHuman();
		vampire.eatHuman();
		
		vampire.fly();
		vampire.fly();
		
		vampire.displayEnergy();
	}

}
